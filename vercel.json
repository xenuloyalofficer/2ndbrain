{
  "version": 2,
  "name": "2nd-brain",
  "buildCommand": "echo 'No build needed - static markdown files'",
  "outputDirectory": ".",
  "routes": [
    {
      "src": "/README.md",
      "dest": "/README.md"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
